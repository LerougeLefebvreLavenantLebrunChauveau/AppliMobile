ponents: {\n      },\n      data() {\n        return {\n          todos: [\n            {\n              id: 1,\n              name: 'tache 1',\n              completed: false\n            },\n            {\n              id: 2,\n              name: 'tache 2',\n              completed: true\n            },\n            {\n              id: 3,\n              name: 'tache 3',\n              completed: true\n            },\n            {\n              id: 4,\n              name: 'tache 4',\n              completed:false\n            }\n          ],\n          newTodo: '',\n          filter: 'all',\n          complete: true,\n          checkToutCompleter: false\n        }\n      },\n      methods: {\n        supprimerTodo(todo){\n          let nb = this.todos.indexOf(todo)\n          this.todos.splice(nb,1)\n        },\n        supprimerComplete(){\n          this.todos.filter(todo => todo.completed).forEach(todo => this.supprimerTodo(todo));\n          this.complete=false;\n        },\n        toutCompleter(){\n          this.todos.forEach(todo => todo.completed = this.checkToutCompleter)\n        },\n        miseAJourTodo(todo){\n          if (todo.completed)\n            this.complete = true;\n          else this.complete = this.todos.filter(todo => todo.completed)\n        }\n      },\n      computed: {\n        reste(){\n          return this.todos.filter(todo => todo.completed).length\n        },\n        filtreTodos(){\n          if (this.filter === 'todo'){\n            return this.todos.filter(todo => !todo.completed).length != 0\n          }\n          return this.todos\n        },\n        siTodos(){\n          return true\n        }\n      }\n    }\n</script>\n\n<style>\n    #app {\n        font-family: Avenir, Helvetica, Arial, sans-serif;\n        -webkit-font-smoothing: antialiased;\n        -moz-osx-font-smoothing: grayscale;\n        text-align: center;\n        color: #2c3e50;\n        margin-top: 60px;\n    }\n    li.todo{\n      color:red;\n    }\n    li.completed{\n      color:green;\n    }\n</style>"],"sourceRoot":""}]}